{
  "name": "gatsby-source-tumblr",
  "version": "0.1.0",
  "description": "Gatsby source plugin for building websites using Tumblr as a data source",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir .",
    "format": "prettier --write 'src/**/*.js' 'README.md'",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run build -- --watch"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-tumblr",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-tumblr.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "tumblr"
  ],
  "author": "Angelo Ashmore",
  "license": "MIT",
  "dependencies": {
    "gatsby-node-helpers": "^0.1.3",
    "node-fetch": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "prettier": "^1.10.2"
  }
}
